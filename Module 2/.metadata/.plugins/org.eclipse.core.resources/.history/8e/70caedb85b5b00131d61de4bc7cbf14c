package ss.week3.hotel;

public class PricedSafe extends Safe implements Item {
	private Safe s;
	private double price;
	
	public void Safe(String pass, double price){
		s = new Safe(pass);
		this.price = price;
	}
	
	public String toString(){
		return "Price: " + price;
	}
	
	public double getAmount(){
		return 42.73;
	}
}
