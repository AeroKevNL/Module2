package ss.week4;

public class Opgave41 {
	static int[] list = {1,-2,3,-4,5,-6};
	
	public static void main(String[] args){
		System.out.println(numberOfNegatives(list));
	}
	
	public static int numberOfNegatives(int[] list){
		int i;
		int count = 0;
		for(i=0; i<list.length; i++){
			if(list[i] < 0){
				count++;
			}
		}
		return count;
	}
}
