package ss.week3.hotel;

public class PricedRoom {
	private int roomNumber;
	private double roomPrice;
	private double safePrice;
	
	public PricedRoom(int num, double roomprice, double safeprice){
		super(num,safeprice);
		this.roomNumber = num;
		this.roomPrice = roomprice;
		this.safePrice = safeprice;
	}
	
	public String toString(){
		return "Room Number: " + roomNumber + "| Room Price: " + roomPrice + " | Safe Price: " + safePrice;
	}
	
	public double getAmount(){
		return 73.42;
	}
} 
